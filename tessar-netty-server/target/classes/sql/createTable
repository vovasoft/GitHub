
--以下是根据周期不同建立了三张新增表，用于新增，活跃等需求
create table NewAddDay
(id int PRIMARY KEY auto_increment
,dateID DATE
,cID VARCHAR (64)
,gID VARCHAR (64)
,sID VARCHAR (64)
,newAddNum INT
,activeNum INT
,loginCount INT
,averageLogin FLOAT
,allPlayerNum INT
);


create table NewAddWeek
(id int PRIMARY KEY auto_increment
,dateID DATE
,cID VARCHAR (64)
,gID VARCHAR (64)
,sID VARCHAR (64)
,newAddNum INT
,activeNum INT
,loginCount INT
,averageLogin FLOAT
,allPlayerNum INT
);


create table NewAddMon
(id int PRIMARY KEY auto_increment
,dateID DATE
,cID VARCHAR (64)
,gID VARCHAR (64)
,sID VARCHAR (64)
,newAddNum INT
,activeNum INT
,loginCount INT
,averageLogin FLOAT
,allPlayerNum INT
);